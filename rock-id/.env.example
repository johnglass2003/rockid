# Environment variables for Rock ID app
# Copy this to .env and fill in your actual API keys

# AI Provider (openai | gemini | custom)
EXPO_PUBLIC_AI_PROVIDER=gemini

# OpenAI API Key (if using OpenAI)
EXPO_PUBLIC_OPENAI_API_KEY=your_openai_api_key_here

# Google Gemini API Key (if using Gemini - FREE tier available)
EXPO_PUBLIC_GEMINI_API_KEY=your_gemini_api_key_here

# Custom Model API Endpoint (if you deploy your own model)
EXPO_PUBLIC_CUSTOM_MODEL_URL=https://your-api.com
